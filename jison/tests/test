program test;
var int xx;
var int yy;
function void main(){
	xx = 10 + 20;
	yy = 10 + xx;
};

program test;
var int xx;
var int yy;
function void main(int zz) {
	var int kk;
	xx = zz + 10 * 20;
	yy = 40;
	kk = xx + 30;
};

program test;
var int xx, int yy;
function void main(int zz, float aa) {
	var int kk, int bb;
	xx = zz + 10 * 20;
	yy = 40;
	kk = xx + 30;
};

program test;
var int xx, int yy, int hello;
function void main(int zz, float aa) {
	var int kk, int bb, float yellow;
	xx = zz + 10 * 20;
	yy = hello + yellow;
	kk = xx + 30;
};

program test;
var int xx;
function void main(int zz, float aa) {
	xx = 10 * 30 - 20;
	write(xx);
};

program test;
function void main() {
	var int x;
	x = 10;
};

program test;
var int xx;
function void main() {
	var int zz;
	zz = 30;
	xx = (zz + 10) * 20;
};


program test;
function void main() {
	var int x;
	if(true) {
		x = 10 + 20;
	};
	x = 30 + 40;
};

program test;
function void main() {
	var int x;
	if(true) {
		x = 10 + 20;
		if(x > 0)	{
			x = 50 * (20 - 10);
		};
	};
	x = 30 + 40;
};

program test;
function void main() {
	var int x, int y;
	if(true) {
		x = 10 + 20;
		if(x > 0)	{
			x = 50 * (20 - 10);
		} else {
			y = x + (30 / 4);
		};
	};
	x = 30 + 40;
};

program test;
function void main() {
	var int x;
	while(x > 0) {
		x = x - 1;
	};
	write(x);
};

program test;
function void main() {
	var int x;
	while(x > 0) {
		if(x == x) {
				x = x - 1;
		};
	};
	write(x);
};
