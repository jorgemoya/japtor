program fibonacci;
function int fibonacci(int n) {
  if(n < 2) {
    return n;
  } else {
    return fibonacci(n-2) + fibonacci(n-1);
  }
}
function void main() {
  write(fibonacci(7));
}

program fibonacci;
function void main() {
  var int number, int a, int b, int f, int i;
  assign number = 7;
  assign i = 2;
  assign a = 0;
  assign b = 1;
  assign f = 1;
  while(i <= number) {
    assign f = a + b;
    assign a = b;
    assign b = f;
    assign i = i + 1;
  }
  write(f);
}
