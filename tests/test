program test;
function void main() {
	var int x;
	if(true) {
		x = 10 + 20;
	}
	x = 30 + 40;
}

program test;
function void main() {
	var int x;
	if(true) {
		x = 10 + 20;
		if(x > 0)	{
			x = 50 * (20 - 10);
		}
	}
	x = 30 + 40;
}

program test;
function void main() {
	var int x, int y;
	if(true) {
		x = 10 + 20;
		if(x > 0)	{
			x = 50 * (20 - 10);
		} else {
			y = x + (30 / 4);
		}
	}
	x = 30 + 40;
}

program test;
function void main() {
	var int x;
	x = 10;
	while(x > 0) {
		write(x);
		x = x - 1;
	}
}

program test;
function void main() {
	var int x;
	x = 10;
	while(x > 0) {
		if(x == x) {
				x = x - 1;
		}
	}
	write(x);
}

program test;
function void main() {
	var int x;
	x = 10;
	while(x > 0) {
		if((x == x)) {
				write(x);
				x = x - 1;
		}
	}
}

program test;
var int a, int b, float f;
function int uno(int c) {
	if(c > 10) {
		c = 10;
		return c;
	}
}
function void main() {
	a = 20;
	b = uno(a);
}

program test;
var int a;
function int uno(int c) {
	if(c > 10) {
		c = 10 + a;
		return c;
	}
}
function void main() {
var int b;
	a = 20;
	b = uno(a);
}

program test;
var int a;
function void uno(int c) {
	if(c > 10) {
		assign c = 10 + a;
		return c;
	}
	return c;
}

function void main() {
var int b;
	assign a = 20;
	uno(a);
	write(a);
}

program factorial;
function int factorial(int x) {
	if(x==0) {
		return 1;
	}
	return x * factorial(x-1);
}
function void main() {
	write(factorial(1));
}

program test;
function void main() {
	var int x[5];
	var int y;
	assign y = 5;
	assign x[2+2] = y + 3;
}

program test;
function void main() {
	var int x[5];
	var int y;
	assign y = 5;
	assign x[1] = 3;
	assign x[2] = 4;
	assign x[3] = 5;
	assign x[2+2] = x[1] + x[2] * x[2+1];
	write(x[4]);
}

program test;
function void main() {
	var int x[5][2];

	assign x[4][1] = 10;
	assign x[3][1] = 5;
	assign x[2][1] = 4;
	assign x[1][1] = x[4][1] + x[3][1] * x[2][1];
	write(x[1][1]);
}
