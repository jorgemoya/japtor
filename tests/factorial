program factorial;
function int factorial(int x) {
	if(x==0) {
		return 1;
	}
	return x * factorial(x-1);
}
function void main() {
	write(factorial(1));
}

program factorial;
function void main() {
  var int number, int factor, int i;
  assign number = 1;
  assign factor = 1;
  assign i = 1;
  while(i <= number) {
    assign factor = factor * i;
    assign i = i + 1;
  }
  write(factor);
}
